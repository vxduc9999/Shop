   <CartItem\r\n                            key={item.product}\r\n                            item={item}\r\n                            qtyChangeHandler={qtyChangeHandler}\r\n                            removeHandler={removeHandler}\r\n                        />\r\n                    ))\r\n                )}\r\n            </div>\r\n            <div className=\"carscreen__right\">\r\n                <div className=\"carscreen__info\">\r\n                    <p>Subtotal ({cartItem.length}) product</p>\r\n                    <p>{getTotal()}VND</p>\r\n                </div>\r\n                <div>\r\n                    <button>Checkout</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default CartScreen;\r\n","C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\components\\CartItem\\CartItem.jsx",[],"C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\pages\\Homepage\\home.page.jsx",[],"C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\components\\Product\\Product.jsx",[],"C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\pages\\Productpage\\product.page.jsx",["152","153"],"import \"./product.style.css\";\r\nimport { useState, useEffect } from \"react\";\r\nimport { useSelector, useDispatch } from \"react-redux\";\r\n\r\n// Actions\r\nimport { getProductDetails } from \"../../redux/actions/productActions\";\r\nimport { addToCart } from \"../../redux/actions/cartActions\";\r\n\r\n\r\n\r\nconst ProductScreen = ({ match, history }) => {\r\n    const [qty, setQty] = useState(1);\r\n    const dispatch = useDispatch();\r\n\r\n    const [urlImages, seturlImages] = useState();\r\n\r\n    const productDetails = useSelector((state) => state.getProductDetails);\r\n\r\n    const { loading, error, product } = productDetails;\r\n\r\n    useEffect(() => {\r\n        if (product && match.params.slug !== product.product_slug) {\r\n            dispatch(getProductDetails(match.params.slug));\r\n        }\r\n    }, [dispatch, product, match]);\r\n\r\n    console.log(product);\r\n    const increaseCounter = () => {\r\n        if (qty + 1 <= product.product_quantity) setQty(qty + 1);\r\n    };\r\n\r\n    const decreaseCounter = () => {\r\n        if (qty - 1 > 0) setQty(qty - 1);\r\n    };\r\n\r\n    const addToCartHandler = () => {\r\n        dispatch(addToCart(product.product_slug, qty));\r\n        history.push(\"/cart\");\r\n    };\r\n\r\n    const handleClickImagesItem = (name) => {\r\n        seturlImages(name);\r\n    };\r\n    console.log(urlImages);\r\n\r\n    return (\r\n        <div className=\"productscreen\">\r\n            {loading ? (\r\n                <h2>Loading...</h2>\r\n            ) : error ? (\r\n                <h2>{error}</h2>\r\n            ) : (\r\n                <>\r\n                    <div className=\"productscreen__left\">\r\n                        <div className=\"left__image\">\r\n                            <img\r\n                                src={`../${urlImages == undefined ? product.product_thumbnail : urlImages\r\n                                    }`}\r\n                                alt=\"product_name\"\r\n                            ></img>\r\n                        </div>\r\n                        <div className=\"left__image__item\">\r\n                            <img\r\n                                onClick={() => handleClickImagesItem(product.product_thumbnail)}\r\n                                className=\"left__image__main\"\r\n                                src={`../${product.product_thumbnail}`}\r\n                                alt=\"product_name\"\r\n                            ></img>\r\n                            {product[\"images\"] == undefined\r\n                                ? \"\"\r\n                                : product[\"images\"].map((x) => (\r\n                                    <img\r\n                                        onClick={() => handleClickImagesItem(x.name)}\r\n                                        src={`../${x.name}`}\r\n                                        alt=\"productdetail_name\"\r\n                                    ></img>\r\n                                ))}\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div className=\"productscreen__right\">\r\n                        <div className=\"right__info\">\r\n                            <p className=\"right__name\">{product.product_name}</p>\r\n                            <div className=\"right__price_couter\">\r\n                                <p className=\"right__price\">\r\n                                    Price: {product.product_price}VND\r\n                  </p>\r\n                                <div className=\"right__couter\">\r\n                                    <button onClick={decreaseCounter}>-</button>\r\n                                    <p value={qty}>{qty}</p>\r\n                                    <button onClick={increaseCounter}>+</button>\r\n                                </div>\r\n                            </div>\r\n\r\n                            <p>Description: {product.product_description}</p>\r\n                            <div className=\"right__add\">\r\n                                <button type=\"button\" onClick={addToCartHandler}>\r\n                                    Add to cart\r\n                  </button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </>\r\n            )}\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default ProductScreen;\r\n\r\n\r\n","C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\redux\\actions\\cartActions.js",[],"C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\frontend\\src\\redux\\actions\\productActions.js",[],"C:\\Users\\Admin\\Desktop\\Mini-Project-Economic\\package\\fro